[sub_resource type="GDScript" id="GDScript_s88ni"]
resource_name = "Front Door"
script/source = "extends Sprite2D

@export var required_key : String = 'Key_0'
@onready var interactable_area = $Area2D

var is_interactable : bool 

signal front_door_attempted
signal missing_key

func _ready():
	interactable_area.connect(\"body_entered\", attempt_to_open_door)
	interactable_area.connect(\"body_exited\", step_away_from_the_Door)
	pass # Replace with function body.

func step_away_from_the_Door(body):
	if body.is_in_group('player'):
		is_interactable = false
	pass
	

func attempt_to_open_door(body):
	if body.is_in_group('player'):
		if body.has_method('show_dialogue_box'):
			var text_to_read : String
			
			body.show_dialogue_box(text_to_read)
		is_interactable = true
		print('close  ' + str(is_interactable))
	
	pass
	

func _input(event):
	
	if Input.is_action_just_pressed(\"Player_interact\") and is_interactable == true:
		for key in K9GlobalItemList.key_list:
			if key == required_key:
				open_door()
		
	pass
	

func open_door():
	print('tried to open the front door')
	emit_signal(\"front_door_attempted\")
	pass
	
"

