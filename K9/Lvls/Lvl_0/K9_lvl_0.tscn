resource_name = "Calendar"
script/source = "extends Sprite2D

var is_interactable : bool 

func _ready():
	$Area2D.connect('body_entered', body_enter)
	$Area2D.connect('body_exited', body_exit)
	pass
	

func body_enter(body):
	if body.is_in_group('player'):
		is_interactable = true
	pass
	

func body_exit(body):
	if body.is_in_group('player'):
		is_interactable = false
	pass

func _input(event):
	if Input.is_action_just_pressed(\"Player_interact\") and is_interactable:
		interact()
	pass
	


func interact():
	var text_to_read : String
	
	var player = get_tree().current_scene.get_node_or_null('PC')
	
	
	text_to_read = \"why does this say '1' ? Is someone pulling a trick on me?\"
	player.show_dialogue_box(text_to_read)
	pass
	
"

