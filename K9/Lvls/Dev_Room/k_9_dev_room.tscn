resource_name = "RNJesusScript"
script/source = "extends Node2D

#var item_spawn_list = {
	#0:
		#load(\"res://K9/Modules/2D/PlainCircle.png\"),
	#1:
		#load(\"res://K9/Modules/2D/SpotLightA.png\"),
	#2:
		#load(\"res://K9/Modules/2D/WoodShedLogoB_4.png\"),
	#}

func _ready():
	#var new_item_p = load(\"res://Global_Modules/GreyBoxIdometric_A.png\")
	#append_item_list(new_item_p)
	
	RNG_spawn_from_list()
	pass
	

func RNG_spawn_from_list(): #the important thing is spawning from a global list we can save
	
	var the_list = K9GlobalItemList.item_spawn_list
	var rng_max = (the_list.size() - 1) #avoids calling higher than dict holds
	var the_chosen = randi_range(0, rng_max)
	
	$Sprite2D.texture = the_list[the_chosen]
	
	print('item list size: ' + str(K9GlobalItemList.item_spawn_list))
	pass
	

func append_item_list(new_item_p):
	#add an item to the global item list
	K9GlobalItemList.item_spawn_list[K9GlobalItemList.item_spawn_list.size() + 1] = new_item_p
	print(str(K9GlobalItemList.item_spawn_list))
	pass
"

